{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
        {
            "source": "/tos",
            "destination": "/tos.html"
        },
        {
            "source": "**",
            "destination": "/index.html"
        }
    ]
  }
}
